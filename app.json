{
  "expo": {
    "name": "mobilebankingapp",
    "slug": "mobilebankingapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fireworks.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.mobilebankingapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cdb0dafb-e6f6-425b-99e4-a559165f0928"
      }
    }
  }
}
